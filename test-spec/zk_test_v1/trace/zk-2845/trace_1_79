  s1 :> [zxid |-> <<1, 2>>, index |-> 2] @@
  s2 :> [zxid |-> <<1, 1>>, index |-> 1] )
/\ rcvBuffer = ( s0 :> (s0 :> <<>> @@ s1 :> <<>> @@ s2 :> <<>>) @@
  s1 :> (s0 :> <<>> @@ s1 :> <<>> @@ s2 :> <<>>) @@
  s2 :> (s0 :> <<>> @@ s1 :> <<>> @@ s2 :> <<>>) )
/\ initialHistory = ( s0 :>
      << [ zxid |-> <<1, 1>>,
           value |-> 101,
           epoch |-> 1,
           ackSid |-> {s0, s1, s2} ],
         [zxid |-> <<1, 2>>, value |-> 102, epoch |-> 1, ackSid |-> {s0}] >> @@
  s1 :>
      << [zxid |-> <<1, 1>>, value |-> 101, epoch |-> 1, ackSid |-> {s1}],
         [zxid |-> <<1, 2>>, value |-> 102, epoch |-> 1, ackSid |-> {s1}] >> @@
  s2 :>
      << [ zxid |-> <<1, 1>>,
           value |-> 101,
           epoch |-> 1,
           ackSid |-> {s0, s1, s2} ] >> )
/\ recorder = [ step |-> 22,
  nPartition |-> 0,
  nConsequentFailure |-> 1,
  noExecute |-> {<<"NodeCrash", s2>>},
  nTimeout |-> 5,
  nCrash |-> 5,
  nTransaction |-> 2,
  maxEpoch |-> 3,
  pc |-> <<"FollowerProcessUPTODATE", s0, s1>>,
  nClientRequest |-> 0 ]
/\ acceptedEpoch = (s0 :> 3 @@ s1 :> 3 @@ s2 :> 3)
/\ connecting = ( s0 :>
      { [connected |-> FALSE, sid |-> s1],
        [connected |-> TRUE, sid |-> s0],
        [connected |-> TRUE, sid |-> s2] } @@
  s1 :>
      { [connected |-> FALSE, sid |-> s2],
        [connected |-> TRUE, sid |-> s0],
        [connected |-> TRUE, sid |-> s1] } @@
  s2 :> {} )
/\ proposalMsgsLog = { [zxid |-> <<1, 1>>, epoch |-> 1, data |-> 101, source |-> s0],
  [zxid |-> <<1, 2>>, epoch |-> 1, data |-> 102, source |-> s0],
  [zxid |-> <<1, 2>>, epoch |-> 3, data |-> 102, source |-> s1] }
/\ lastCommitted = ( s0 :> [zxid |-> <<1, 2>>, index |-> 2] @@
  s1 :> [zxid |-> <<1, 2>>, index |-> 2] @@
  s2 :> [zxid |-> <<0, 0>>, index |-> 0] )
/\ daInv = [ stateConsistent |-> TRUE,
  proposalConsistent |-> FALSE,
  commitConsistent |-> FALSE,
  ackConsistent |-> TRUE,
  messageLegal |-> TRUE ]
/\ currentEpoch = (s0 :> 3 @@ s1 :> 3 @@ s2 :> 1)
/\ learners = (s0 :> {} @@ s1 :> {s0, s1} @@ s2 :> {})
/\ tempMaxEpoch = (s0 :> 1 @@ s1 :> 3 @@ s2 :> 0)
/\ leaderOracle = s1
/\ status = (s0 :> ONLINE @@ s1 :> ONLINE @@ s2 :> ONLINE)
/\ state = (s0 :> FOLLOWING @@ s1 :> LEADING @@ s2 :> LOOKING)
/\ partition = ( s0 :> (s0 :> FALSE @@ s1 :> FALSE @@ s2 :> FALSE) @@
  s1 :> (s0 :> FALSE @@ s1 :> FALSE @@ s2 :> FALSE) @@
  s2 :> (s0 :> FALSE @@ s1 :> FALSE @@ s2 :> FALSE) )
