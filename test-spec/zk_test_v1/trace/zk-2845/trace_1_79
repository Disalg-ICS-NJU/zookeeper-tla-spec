        [connected |-> TRUE, sid |-> s2] } @@
  s1 :> {[connected |-> TRUE, sid |-> s0], [connected |-> TRUE, sid |-> s1]} @@
  s2 :> {} )
/\ leaderAddr = (s0 :> s1 @@ s1 :> NullPoint @@ s2 :> NullPoint)
