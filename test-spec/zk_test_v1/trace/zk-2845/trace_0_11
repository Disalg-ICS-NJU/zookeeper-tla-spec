/\ recorder = [ nPartition |-> 1,
  nConsequentFailure |-> 2,
  noExecute |->
      { <<"PartitionStart", {s1, s2}>>,
        <<"PartitionRecover", {s1, s2}>>,
        <<"NodeCrash", s0>> },
  nTimeout |-> 1,
  nCrash |-> 1,
  nTransaction |-> 0,
  maxEpoch |-> 0,
  pc |-> <<"PartitionStart", s1, s2>>,
  nClientRequest |-> 0 ]
